<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <android.support.v4.view.ViewPager
        android:id="@+id/analyzemanagment_viewPager"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/analyzemanagment_appBarLayout" />

    <android.support.design.widget.AppBarLayout
        android:id="@+id/analyzemanagment_appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.example.huhep.litepaltest.CustomToolbar
            android:id="@+id/analyzemanagment_toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <android.support.design.widget.TabLayout
            android:id="@+id/analyzemanagment_tabLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/textViewBackground"
            app:tabIndicatorColor="@color/lightBlue2"
            app:tabIndicatorHeight="3dp"
            app:tabMode="scrollable"
            app:tabSelectedTextColor="@color/deepDeepDark"
            app:tabTextColor="@color/deepDark" />
    </android.support.design.widget.AppBarLayout>

</android.support.constraint.ConstraintLayout>